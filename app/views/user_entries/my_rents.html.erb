<div id="my_rents" >
	<h1>Ваши объявления об аренде:</h1>
	<table id="flat_and_rooms_moscow_table" class="search_result_table">
		<tr>
			<th id="address_header">Адрес</th>
			<th id="type_header">Тип помещения</th>
			<th id="price_header">Цена руб/мес</th>
			<th id="commission_header">Коммиссия:</th>
			<th id="total_area_header"> Площадь суммарная</th>
			<th id="kitchen_area_header">Площадь кухни</th>
			<th id="misc">Доп. инфо</th>
			<th id="contacts_header">Контакты</th>
			<th id="comment_header">Комментарий</th>
		</tr>
		<%= render(:partial => "entries/rent_entry" , :collection => @entries) %>
	</table>
</div>
