<h1>Editing address_highway</h1>

<% form_for(@address_highway) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :address_id %><br />
    <%= f.text_field :address_id %>
  </p>
  <p>
    <%= f.label :highway_id %><br />
    <%= f.text_field :highway_id %>
  </p>
  <p>
    <%= f.label :distance_to_mkad %><br />
    <%= f.text_field :distance_to_mkad %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @address_highway %> |
<%= link_to 'Back', address_highways_path %>